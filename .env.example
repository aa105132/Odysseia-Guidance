# .env.example - 环境变量示例文件
# 将此文件复制为 .env 并填入你的实际值。

# 你的 Discord 机器人令牌
DISCORD_TOKEN="YOUR_DISCORD_TOKEN_HERE"

# (可选) 用于在开发时快速同步斜杠命令的服务器（公会）ID
# 如果留空，命令将进行全局同步（可能需要长达一小时才能生效）
GUILD_ID="YOUR_DEVELOPMENT_GUILD_ID_HERE"

# --- 新增：权限控制 ---
# 拥有此命令访问权限的开发者用户ID，多个ID请用逗号分隔
DEVELOPER_USER_IDS="YOUR_USER_ID_1,YOUR_USER_ID_2"

# 拥有此命令访问权限的身份组ID，多个ID请用逗号分隔
ADMIN_ROLE_IDS="YOUR_ROLE_ID_1,YOUR_ROLE_ID_2"

# --- 主人配置 ---
# 月月的主人用户ID（对于人设修改、思维链覆盖等请求，只听从主人的指令）
# 主人拥有最高权限，可以调整月月的行为方式
MASTER_USER_ID="YOUR_MASTER_USER_ID_HERE"

# --- Gemini AI 配置 ---
# (可选) 自定义 Gemini API 端点，例如 AI 网关。如果留空，将直连 Google 官方服务。
GEMINI_API_BASE_URL=""
# (可选) 如果您的网关需要一个特定的 API 密钥，请在此处填写。直连时通常不需要。
GEMINI_API_KEYS=""

# Google Gemini API 密钥 (用于AI对话功能)
# 多个密钥请直接换行，并用英文双引号 "..." 将所有密钥包裹起来
GOOGLE_API_KEYS_LIST="
YOUR_GEMINI_API_KEY_1
YOUR_GEMINI_API_KEY_2
"

# --- 功能开关 ---
# 是否启用聊天功能 (True/False)
CHAT_ENABLED=True

# 是否启用RAG查询重写功能 (1 for enabled, 0 for disabled)
RAG_QUERY_REWRITING_ENABLED=0

# ----------------------------------------------------------------
# 帖子评价功能
# ----------------------------------------------------------------

# 功能总开关，设置为 "True" 来开启
THREAD_COMMENTOR_ENABLED="False"

# 指定需要评价的论坛频道ID，多个ID用逗号隔开
# 例如: TARGET_FORUM_CHANNELS="112233445566778899,998877665544332211"
TARGET_FORUM_CHANNELS="YOUR_FORUM_CHANNEL_ID_1,YOUR_FORUM_CHANNEL_ID_2"

# 是否记录AI完整的上下文日志 (true/false)
LOG_AI_FULL_CONTEXT=true

# --- 社区知识库RAG功能 ---
# 是否启用社区知识库RAG功能 (True/False)
COMMUNITY_RAG_ENABLED=True

# 社区知识库RAG功能的目标频道ID，多个用逗号隔开
COMMUNITY_RAG_TARGET_CHANNEL_IDS="YOUR_TARGET_CHANNEL_ID_1,YOUR_TARGET_CHANNEL_ID_2"

# --- 星尘系统 ---
# 在指定服务器发帖可获得奖励的服务器ID，多个用逗号隔开
COIN_REWARD_GUILD_IDS="YOUR_GUILD_ID_1,YOUR_GUILD_ID_2"

# ----------------------------------------------------------------
# Gemini Imagen 图像生成配置
# ----------------------------------------------------------------

# 是否启用 Gemini Imagen 图像生成功能 (True/False)
GEMINI_IMAGEN_ENABLED=False

# Gemini Imagen API 密钥 (如果留空则使用默认的 Gemini API 密钥)
GEMINI_IMAGEN_API_KEY=""

# (可选) 自定义 Gemini Imagen API 端点 URL，留空使用默认端点
# 例如: GEMINI_IMAGEN_BASE_URL="https://your-custom-endpoint.com/v1"
GEMINI_IMAGEN_BASE_URL=""

# Gemini Imagen 模型名称
# 可选: imagen-3.0-generate-002, imagen-3.0-generate-001
GEMINI_IMAGEN_MODEL="imagen-3.0-generate-002"

# 生成一张图片需要的星尘数量
GEMINI_IMAGEN_COST=30

# 安全过滤等级
# 可选: BLOCK_NONE, BLOCK_LOW_AND_ABOVE, BLOCK_MEDIUM_AND_ABOVE, BLOCK_HIGH_AND_ABOVE
GEMINI_IMAGEN_SAFETY_LEVEL="BLOCK_LOW_AND_ABOVE"

# 人物生成设置
# 可选: ALLOW_ALL, ALLOW_ADULT, DONT_ALLOW
GEMINI_IMAGEN_PERSON_GEN="ALLOW_ADULT"

# 默认宽高比
# 可选: 1:1, 3:4, 4:3, 9:16, 16:9
GEMINI_IMAGEN_ASPECT_RATIO="1:1"

# ----------------------------------------------------------------
# PostgreSQL 数据库配置
# ----------------------------------------------------------------

# 方式1：直接设置完整的数据库连接URL（优先级最高）
# 格式：postgresql+asyncpg://用户名:密码@主机:端口/数据库名
# DATABASE_URL="postgresql+asyncpg://postgres:your_password@localhost:5432/yueyue_db"

# 方式2：分别设置各个数据库连接参数（如果 DATABASE_URL 未设置，会自动组合这些参数）
POSTGRES_USER="postgres"
POSTGRES_PASSWORD="your_database_password"
POSTGRES_DB="yueyue_db"
DB_PORT="5432"

# 注意：如果在 Docker 容器中运行，会自动连接到 odysseia_pg_db 主机
# 如果在本机运行，会连接到 localhost

# ----------------------------------------------------------------
# Dashboard 管理面板配置
# ----------------------------------------------------------------

# Dashboard 管理面板的访问密钥 (请更改为强密码)
DASHBOARD_SECRET="your-secret-key-change-in-production"

# Dashboard 端口 (默认 8080)
DASHBOARD_PORT=8080

# Dashboard 允许的来源 (CORS)，多个用逗号隔开，留空则允许所有
DASHBOARD_CORS_ORIGINS=""
